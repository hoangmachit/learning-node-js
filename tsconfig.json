{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2023",
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@prisma/client": ["./generated/prisma/client"]
    }
  },
  "files": ["src/type.d.ts"],
  "include": ["src/**/*"]
}
